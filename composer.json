{
    "name": "credevator/lipik",
    "description": "Lipik installation profile for Drupal 8/9.",
    "type": "drupal-profile",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Aman Srivastava",
            "role": "Developer"
        }
    ],
    "repositories": {
        "asset": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "require": {
        "drupal/admin_toolbar": "^3.0",
        "drupal/better_exposed_filters": "^5.0",
        "drupal/core": "^8.8 || ^9.0",
        "drupal/entity_reference_display": "^1.2",
        "drupal/entity_reference_revisions": "^1.3",
        "drupal/facets": "^1.0",
        "drupal/features": "^3.5",
        "drupal/field_group": "^3.0",
        "drupal/link_attributes": "^1.1",
        "drupal/linkit": "^6.0-beta",
        "drupal/menu_link_attributes": "^1.0",
        "drupal/paragraphs": "^1.1",
        "drupal/pathauto": "^1.0",
        "drupal/redirect": "^1.0-beta1",
        "drupal/search_api": "^1.8",
        "drupal/simple_sitemap": "^3.3",
        "drupal/svg_image": "^1.8"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
